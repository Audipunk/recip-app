{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","airbnb/hooks","eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-underscore-dangle": "off",
      "no-trailing-spaces":"off",
      "react/jsx-closing-bracket-location": "off",
      "quotes": "off"
    }
}
